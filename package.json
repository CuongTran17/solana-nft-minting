{
  "name": "solana-nft-minting",
  "version": "1.0.0",
  "description": "NFT Minting on Solana",
  "type": "module",
  "scripts": {
    "build": "anchor build",
    "deploy": "anchor deploy",
    "mint": "ANCHOR_PROVIDER_URL=https://api.devnet.solana.com ANCHOR_WALLET=~/.config/solana/id.json ts-node --esm -r dotenv/config tests/mint-nft-simple.ts",
    "mint:interactive": "ANCHOR_PROVIDER_URL=https://api.devnet.solana.com ANCHOR_WALLET=~/.config/solana/id.json ts-node --esm -r dotenv/config tests/mint-interactive.ts"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.29.0",
    "@solana/spl-token": "^0.3.9",
    "@solana/web3.js": "^1.87.6",
    "axios": "^1.12.2",
    "dotenv": "^16.6.1",
    "form-data": "^4.0.4",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/prompts": "^2.4.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
